<?php

	
		$X = new DatabaseCon;
		$con = $X->connect();
		$X->selectDB("zandu",$con);
		
		$stmnt = "select bot_id from bot where status=1" ;
		$result = $X->fireQuery($stmnt);
		
		while($row = mysql_fetch_array($result))
		{
				echo "<li><h3>{$row['bot_id']}</h3></li>";
		}
		
		$X->closeConnection($con);
?>